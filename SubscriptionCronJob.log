            $('#alrt_class_cnt').removeClass('alert-success').addClass('alert-danger');
            $('#alrt_class_cnt').html('Last name should not have any numeric value !!');
            $('#msg_block_cnt').removeClass('hidden');
            return false;

        }
        //return false;
        // alert(phone_number_code);
        $.ajax({
            url: url,
            type: "POST", 
            dataType: "json",
